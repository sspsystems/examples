{
    "name": "ssp/plugin-template-php",
    "description": "Minimal SSP POS Plugin Template - PHP",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "scripts": {
        "start": "php -S localhost:3000 index.php"
    }
}
